<% @title = "Show Employee" %>
<div class="page-header">
	<h1><%= @employee.last_name %>, <%= @employee.first_name %> (Employee) <%= link_to "Edit Employee", edit_admin_employee_path(@employee), :class => "btn btn-primary pull-right" %></h1>
</div>


<dl class="dl-horizontal">
	<dt>Title</dt>
	<dd><%= @employee.title %></dd>
	<dt>Last Name</dt>
	<dd><%= @employee.last_name %></dd>
	<dt>First Name</dt>
	<dd><%= @employee.first_name %></dd>
	<dt>Email</dt>
	<dd><%= @employee.email %></dd>
	<dt>Active</dt>
	<dd><%= @employee.active %></dd>
	<dt>Administrator</dt>
	<dd><%= @employee.administrator %></dd>
	<dt>Dispensing Optician</dt>
	<dd><%= @employee.dispensing_optician %></dd>
</dl>
